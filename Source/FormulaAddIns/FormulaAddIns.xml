<?xml version="1.0" encoding="utf-8"?>
<!-- This file along with its dll needs to be installed to ...InstallDir...\RuntimeData\FormulaAddIn to run. -->
<FormulaAddIn>
  <Function>
    <Name>AverageNonNull</Name>
    <NumParams variable="true">1</NumParams>
    <Category>Math</Category>
    <InsertText>AverageNonNull(n1, ...)</InsertText>
    <Description>Averages the non-null entries in the given list.</Description>
    <Dll>
      <Name>FormulaAddIns.dll</Name>
      <EntryPoint>AverageNonNull</EntryPoint>
    </Dll>
  </Function>
  <Function>
    <Name>Coalesce</Name>
    <NumParams variable="true">1</NumParams>
    <Category>Specialized</Category>
    <InsertText>Coalesce(n1, ...)</InsertText>
    <Description>Returns the first non-null entry, or null if all entries are null.</Description>
    <Dll>
      <Name>FormulaAddIns.dll</Name>
      <EntryPoint>Coalesce</EntryPoint>
    </Dll>
  </Function>
</FormulaAddIn>
